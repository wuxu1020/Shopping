<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="agent" extends="struts-default,json-default"
		namespace="/">
		<action name="*_*" class="shopping.action.{1}Action" method="{2}">
			<result name="LoginSuccess">/Homepage.jsp</result>
			<result name="Loginfailed">/UserLogin.jsp</result>
			<result name="RegisterSuccess">/UserLogin.jsp</result>
			<result name="RegisterFailed">/UserRegister.jsp</result>
			<result name="AdminLoginSuccess">/AdminManage.jsp</result>
			<result name="AdminLoginFailed">/AdminLogin.jsp</result>
			<result name="AdminUser">/AdminUser.jsp</result>
			<result name="UpProduct">/AdminProduct.jsp</result>
			<result name="AjaxResult" type="json"></result>
			<result name="UserProductShow">/UserShow.jsp</result>
			<result name="editProduct">/AdminProduct.jsp</result>
			<result name="ToCart">/ShoppingCar.jsp</result>
			<result name="ToUserOrder">/UserOrder.jsp</result>
			<result name="ToAdminOrder">/AdminOrder.jsp</result>
		</action>
	</package>

	
</struts>    
